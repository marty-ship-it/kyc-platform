{
  "name": "kyc-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4287",
    "build": "prisma generate && next build",
    "build:prod": "cp prisma/schema-render.prisma prisma/schema.prisma && prisma generate && next build",
    "build:render": "cp prisma/schema-render.prisma prisma/schema.prisma && npx prisma generate && npx prisma db push && npx tsx prisma/seed.ts && next build",
    "start": "next start",
    "start:render": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "db:reset": "npx prisma migrate reset --force && npx prisma db seed",
    "db:seed:render": "tsx prisma/seed.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/bcrypt": "^6.0.0",
    "@types/pg": "^8.15.5",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
